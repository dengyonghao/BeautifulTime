<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.order.student.model.Books" table="books" schema="dbopenfire">
        <id name="book_id" type="java.lang.String">
            <column name="BOOK_ID" length="20" />
            <generator class="assigned" />
        </id>
        
        <property name="book_name" type="java.lang.String">
            <column name="BOOK_NAME"  not-null="true" />
        </property>
        
        <property name="price" type="java.lang.String">
            <column name="PRICE" />
        </property>
        
      </class>  
</hibernate-mapping>
